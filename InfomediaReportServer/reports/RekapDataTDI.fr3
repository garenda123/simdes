    <TfrxADOQuery Name="frxDBData" UserName="frxDBData" CloseDataSource="True" FieldAliases.Text="" SQL.Text="SELECT&#13;&#10;month(x.tanggal_sk) as bulan,&#13;&#10;year(x.tanggal_sk) as tahun,&#13;&#10;    d.company_name as nama_perusahaan,&#13;&#10;    a.address + ' ' + f.desa + ' Kec. ' + f.kecamatan + ' Kota Banjar' as alamat_perusahaan,&#13;&#10;    c.pemohon_name as nama_penanggung_jawab,&#13;&#10;    a.nilai_invest,&#13;&#10;    a.kelompok_id,                                             &#13;&#10;    a.jenis_usaha,&#13;&#10;    e.tdi_type,&#13;&#10;    x.tanggal_sk as tanggal_terbit,&#13;&#10;    x.nomor_sk as no_sertifikat                                          &#13;&#10;    &#13;&#10; FROM t_permit_TDI a&#13;&#10;   &#13;&#10; LEFT OUTER JOIN m_company_detail x ON a.permit_id = x.permit_id&#13;&#10; LEFT OUTER JOIN m_comp_daftar b ON x.daftar_id = b.daftar_id     &#13;&#10; LEFT OUTER JOIN m_company c ON b.company_id = c.company_id&#13;&#10; LEFT OUTER JOIN m_comp d ON c.comp_id = d.comp_id                                                                                                                                                    &#13;&#10; LEFT OUTER JOIN d_tdi_type e ON a.tdi_type_id  = e.tdi_type_id&#13;&#10; LEFT OUTER JOIN d_Desa f ON a.desa_id = f.desa_id&#13;&#10;&#13;&#10;WHERE month(x.tanggal_sk) = :bulan  AND year(x.tanggal_sk) = :tahun                                                                                                    &#13;&#10;&#13;&#10;ORDER BY x.nomor_sk&#13;&#10;                " CommandTimeout="30" Database="ADODatabase1" PropData=""/>
