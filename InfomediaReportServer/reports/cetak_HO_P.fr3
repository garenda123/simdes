    <TfrxADOQuery Name="frxDBData" UserName="frxDBData" CloseDataSource="True" FieldAliases.Text="" SQL.Text="SELECT&#13;&#10;a.permit_id,&#13;&#10;dbo.tanggal(x.tanggal_sk) as tanggal_sk,&#13;&#10;dbo.tanggal(a.ig_tanggal) as ig_tanggal,&#13;&#10;dbo.tanggal(x.tanggal_habis_berlaku) as tanggal_habis_berlaku,                                                                                                                          &#13;&#10;c.company_type_label, y.company_name,&#13;&#10;x.retribusi,&#13;&#10;a.address + ISNULL(' ' + L.desa + ' Kec. ' + L.kecamatan + ' Kota Banjar' ,'') as alamat_lokasi_izin,&#13;&#10;m.luas, m.business_kind,  m.tahun_keterlambatan+1 as tahun_keterlambatan,&#13;&#10;a.*,&#13;&#10;b.pemohon_name, b.pemohon_address, c.*,b.pemohon_npwp,g.kode&#13;&#10;,g.ho_type,h.kode_rekening,i.index_lokasi,h.index_gangguan, g.koef&#13;&#10;,x.*&#13;&#10;, CASE&#13;&#10; 	WHEN (datediff(day,x.tanggal_perhitungan,x.tanggal_bayar) / 31) &#60; 0 THEN 0&#13;&#10;    WHEN (datediff(day,x.tanggal_perhitungan,x.tanggal_bayar) / 31) &#62;= 0 THEN (datediff(day,x.tanggal_perhitungan,x.tanggal_bayar) / 31)&#13;&#10; ELSE 0&#13;&#10; END&#13;&#10; as mmdenda&#13;&#10;&#13;&#10;,k.nationality,dateadd(year,m.tahun_keterlambatan,a.ig_tanggal) as exp&#13;&#10;,(x.retribusi + x.denda) as totaldenda&#13;&#10;,b.input_date&#13;&#10;,case&#13;&#10;       when a.tahun_keterlambatan &#60; 1 then ''&#13;&#10;       else ' x ' + cast(a.tahun_keterlambatan+1 as varchar) + ' (' + cast(a.tahun_keterlambatan as varchar) + ' tahun keterlambatan + 1 tahun berjalan' +')'&#13;&#10;      end    as ket_terlambat&#13;&#10;&#13;&#10;FROM t_permit_ho_p a&#13;&#10;LEFT OUTER JOIN m_company_detail x ON a.permit_id = x.permit_id&#13;&#10;LEFT OUTER JOIN m_comp_daftar z ON x.daftar_id = z.daftar_id&#13;&#10;LEFT OUTER JOIN m_company b ON  z.company_id = b.company_id&#13;&#10;LEFT OUTER JOIN m_comp y ON b.comp_id = y.comp_id&#13;&#10;LEFT OUTER JOIN d_company_type c ON  y.company_type_id = c.co