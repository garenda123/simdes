    <TfrxADOQuery Name="frxDBData" UserName="frxDBData" CloseDataSource="True" FieldAliases.Text="" SQL.Text="SELECT&#13;&#10;z.company_name,&#13;&#10;z.pusat_address  + ' ' + k.desa + ' ' + 'Kec.' + k.kecamatan + ' ' + 'Kota Banjar' as alamat_perusahaan,    &#13;&#10;c.pemohon_name,c.pemohon_address  + ' ' + k.desa + ' ' + 'Kec.' + k.kecamatan + ' ' + 'Kota Banjar' as alamat_pemohon,    &#13;&#10;a.nama_penanggung_jawab1,&#13;&#10;a.penanggung_jawab_teknis,&#13;&#10;a.pjt_nomor,&#13;&#10;a.pjt_tanggal_terbit,&#13;&#10;a.pjt_masa_berlaku,&#13;&#10;(substring(a.nomor_sbu,1,1))+'-'+(substring(a.nomor_sbu,2,4))+'-'+(substring(a.nomor_sbu,6,2))+'-'+&#13;&#10;(substring(a.nomor_sbu,8,3))+'-'+(substring(a.nomor_sbu,11,1))+'-'+(substring(a.nomor_sbu,12,2))+'-'+&#13;&#10;(substring(a.nomor_sbu,14,6)) as nomor_sbu,&#13;&#10;a.tanggal_terbit_sbu,&#13;&#10;a.masa_berlaku_sbu,&#13;&#10;x.nomor_sk,&#13;&#10;x.tanggal_SK,&#13;&#10;m.jenis_usaha,&#13;&#10;x.nomor_blanko_sk                                &#13;&#10; &#13;&#10; FROM t_permit_IUJK a&#13;&#10; LEFT OUTER JOIN m_company_detail x ON a.permit_id = x.permit_id&#13;&#10; LEFT OUTER JOIN m_comp_daftar y ON x.daftar_id = y.daftar_id                                                                                               &#13;&#10; LEFT OUTER JOIN m_company c ON y.company_id = c.company_id&#13;&#10; LEFT OUTER JOIN m_comp z ON c.comp_id = z.comp_id        &#13;&#10; LEFT OUTER JOIN d_DESA k ON c.desa_id = k.desa_id &#13;&#10; LEFT OUTER JOIN d_IUJK_jenis_usaha m ON a.jenis_usaha_id = m.jenis_usaha_id                            &#13;&#10;&#13;&#10; WHERE month(x.tanggal_sk) = :bulan  AND year(x.tanggal_sk) = :tahun&#13;&#10;&#13;&#10; ORDER BY x.nomor_sk                " CommandTimeout="30" Database="ADODatabase1" PropData=""/>
